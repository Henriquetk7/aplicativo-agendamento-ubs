{
  "expo": {
    "name": "UBS Agendamento",
    "slug": "app-agendamento-ubs",
    "version": "1.0.0",
    "scheme": "app-agendamento-UBS",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F0F1F2"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F0F1F2"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.henriquetk.appagendamentoubs"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f26d0a70-a42c-4ee1-91f7-df0c4ca59135"
      }
    }
  }
}
